{
   "name": "Excited Gem",
   "background": {
      "scripts": [ "js/jquery.min.js","js/background.js","js/common.js" ]
   },
   "content_scripts": [ {
      "js": [ "js/react/dist/react.js","js/react-dom/dist/react-dom.js","js/jquery.min.js","js/common.js","js/content.js" ]
      // , "css":["css/bootstrap.css"]
      , "matches": [ "<all_urls>"]
      ,"all_frames":true   //Defaults to false,
   } ],
   "browser_action": {
      "default_icon": {
         "19": "images/icon19.png",
         "38": "images/icon38.png"
      },
      "default_title":"Excited Gem"
      // ,"default_popup":"popup.html"
   },
   "commands": {
      "display-onetab": {
         "description": "Display OneTab",
         "suggested_key": {
            "default": "Ctrl+Shift+E"
         }
      },
      "send-tabs-on-the-left-to-onetab": {
         "description": "Send tabs on the left to OneTab",
         "suggested_key": {
            "default": "Ctrl+Shift+Left"
         }
      },
      "send-tabs-on-the-right-to-onetab": {
         "description": "Send tabs on the right to OneTab",
         "suggested_key": {
            "default": "Ctrl+Shift+Right"
         }
      },
      "send-all-tabs-except-this-to-onetab": {
         "description": "Send all tabs except this tab to OneTab",
         "suggested_key": {
            "default": "Ctrl+Shift+Up"
         }
      },
      "send-current-tab-to-onetab": {
         "description": "Send the current tab to OneTab"
      }
   },
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com 'unsafe-eval'; object-src 'self'",
   "description": "A try to make a better version of Onetab",
   "icons": {
      "128": "images/extension-icon128.png",
      "16": "images/extension-icon16.png",
      "48": "images/extension-icon48.png"
   },
   // "key": "MIGfMA0GCSqGSIb3DQEBAQUAA4GNADCBiQKBgQDs2r/nSa4MRBoDT1pBx+Q9gI0JaTBaIrsD4uKT9oA4HUF2xboeOA1BDw3lmoJ4G8BL0tAbJTcEnxlkTsCY7C413MojaeDZy4pza3RJqkMAKiJisl8t8eiP+c0ox3l1syb1xmI7EEIouXIxlCX8LGoguB0AuWtunGLCjiLoM2vJLQIDAQAB",
   "manifest_version": 2,
  "options_ui": {
    // Required.
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": true
   },
   "permissions": [ "debugger","tabs","idle","<all_urls>","webRequest", "webRequestBlocking", "contextMenus","webNavigation","storage","notifications"],
   "version": "0.7",
    "web_accessible_resources": [
    "images/extension-icon48.png",
    // "fonts/*",
    "options.html",
    "manifest.json"

  ],
  "externally_connectable":{
    "matches": ["*://localhost/*","*://127.0.0.1/*"],
    "ids": ["*"],
    "accepts_tls_channel_id": false
  }
}
